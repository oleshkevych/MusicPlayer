<?xml version="1.0" encoding="utf-8"?>
<layer-list
xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="15dp"
                />
            <solid
                android:color="@color/checkedBackground"/>
            <stroke
                android:width="1px"
                android:color="@color/checkedBorderColor"/>
        </shape>
    </item>
</layer-list>
